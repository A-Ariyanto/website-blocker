{
  "name": "Ariyanto's website blocker extension",
  "description": "A chrome extension that blocks websites",
  "version": "1.0",
  "manifest_version": 3,
  "icons": {
    "16": "site_blocker_icon_16.png",
    "32": "site_blocker_icon_32.png",
    "48": "site_blocker_icon_48.png",
    "128": "site_blocker_icon_128.png"
  },
  "action": {
    "default_popup": "default.html",
    "default_icon": {
      "16": "site_blocker_icon_16.png",
      "32": "site_blocker_icon_32.png"
    }
  },
  "permissions": ["storage", "declarativeNetRequest"],
  "host_permissions": ["<all_urls>"],
  "background": "background.js"
}
